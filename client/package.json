{
  "name": "red",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run generate && npm run build:nuxt",
    "build:nuxt": "npx --no nuxt build",
    "build:dev": "npm run build:nuxt:dev",
    "build:nuxt:dev": "npx --no nuxt build --envName development",
    "dev": "npm run generate && npm run dev:nuxt",
    "dev:nuxt": "nuxt dev",
    "cleanup": "nuxt cleanup",
    "preview": " npm run build:nuxt && nuxt preview",
    "preview:dev": " npm run build:nuxt:dev && nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "TZ=UTC npm run test:unittests && npm run test:types && npm run test:lint",
    "test:lint": "eslint .",
    "test:lint:fix": "eslint . --fix",
    "test:types": "npx nuxi typecheck",
    "test:unittests": "vitest run",
    "test:unittests:watch": "vitest",
    "test:story": "npm run story:build && LOST_PIXEL_DISABLE_TELEMETRY=1 npx --no lost-pixel docker",
    "test:story:view": "npx --no -c 'vite dev ./visual-regression-viewer'",
    "test:story:approve": "npm run story:build && LOST_PIXEL_DISABLE_TELEMETRY=1 npx --no lost-pixel docker update",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview --port 4567",
    "generate": "npm run generate:api-red",
    "generate:api-red": "npx --no apigen-ts http://localhost:8000/api/schema/ --inline-enums --parse-dates ./generated/red-client.ts"
  },
  "//": "If updating lost-pixel sync the version to the visual-regression-test.yml and post-create.sh or rendering / filenames generated etc may be different",
  "dependencies": {
    "@histoire/plugin-nuxt": "1.0.0-alpha.2",
    "@histoire/plugin-vue": "1.0.0-alpha.2",
    "@internationalized/date": "3.8.1",
    "@nuxt/content": "3.5.1",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.13.0",
    "@nuxtjs/color-mode": "3.4.4",
    "@pinia/nuxt": "0.11.1",
    "@tailwindcss/vite": "4.1.7",
    "@vueuse/core": "13.1.0",
    "apigen-ts": "1.1.0",
    "core-js": "3.40.0",
    "feed": "4.2.2",
    "histoire": "1.0.0-alpha.2",
    "lodash-es": "4.17.21",
    "luxon": "3.5.0",
    "nitropack": "2.10.4",
    "nuxt": "3.15.4",
    "pinia-plugin-persistedstate": "4.3.0",
    "reka-ui": "2.2.1",
    "remark-heading-id": "1.0.1",
    "sharp": "0.34.2",
    "simple-git": "3.27.0",
    "tailwindcss": "4.1.7",
    "typesense-instantsearch-adapter": "2.9.0",
    "vue": "3.5.13",
    "vue-instantsearch": "4.20.8"
  },
  "devDependencies": {
    "@babel/runtime": "7.27.1",
    "@nuxt/test-utils": "3.19.1",
    "@playwright/test": "1.49.1",
    "@types/lodash-es": "4.17.12",
    "@types/luxon": "3.4.2",
    "@types/remark-heading-id": "1.0.0",
    "@vue/test-utils": "2.4.6",
    "fast-xml-parser": "4.5.1",
    "happy-dom": "15.11.7",
    "lost-pixel": "3.22.0",
    "playwright": "1.49.1",
    "playwright-core": "1.49.1",
    "vitest": "2.1.8",
    "vue-tsc": "2.0.29"
  },
  "peerDependencies": {
    "typescript": "5.5.4"
  }
}
